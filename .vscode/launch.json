{
  "configurations": [
    {
      "name": "CV: Build CV and Resume",
      "type": "node-terminal",
      "request": "launch",
      "command": "xelatex cv.tex; xelatex resume.tex",
      "cwd": "${workspaceFolder}",
      "env": {
        "SOURCE_DATE_EPOCH": "1"
      },
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "CV: Build CV",
      "type": "node-terminal",
      "request": "launch",
      "command": "xelatex cv.tex",
      "cwd": "${workspaceFolder}",
      "env": {
        "SOURCE_DATE_EPOCH": "1"
      },
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "CV: Build Resume",
      "type": "node-terminal",
      "request": "launch",
      "command": "xelatex resume.tex",
      "cwd": "${workspaceFolder}",
      "env": {
        "SOURCE_DATE_EPOCH": "1"
      },
      "internalConsoleOptions": "neverOpen"
    }
  ]
}